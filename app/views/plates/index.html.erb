<div id="plates-index">
  <% if current_user %>
  <h2 class="text-center">Welcome, <%= current_user.username %>!</h2>
  <% end %>

  <% @all_plates.each do |plate| %>
  <div class="wall-photo">
    <%= link_to image_tag(plate.url), plate_path(plate), id: "#{plate.id}", class: "index-photo" %>
  </div>
  <% end %>
</div>
